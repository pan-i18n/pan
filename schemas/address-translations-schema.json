{
    "$schema": "http://json-schema.org/schema#",
    "$id": "http://github.com/pan-i18n/pan-data/schemas/address-translations-schema.json",
    "title": "address-translations",
    "type": "object",
    "definitions": {
        "fieldMetadata": {
            "type": "object",
            "description": "Metadata about a particular address field for constructing an input UI.",
            "properties": {
                "label": {
                    "type": "string",
                    "description": "The primary label for this field."
                },
                "placeholder": {
                    "type": "string",
                    "description": "The placeholder text (if any) to display in the input field until the user clicks in it."
                },
                "toolTip": {
                    "type": "string",
                    "description": "The tool tip text (if any) to display when a user hovers over this input field."
                }
            },
            "required": ["label"]
        }
    },
    "properties": {
        "fields": {
            "type": "object",
            "description": "A mapping between fields and translations for those fields",
            "properties": {
                "streetAddress": {
                    "$ref": "#/definitions/fieldMetadata",
                    "description": "The street and premises number of the address"
                },
                "building": {
                    "$ref": "#/definitions/fieldMetadata",
                    "description": "The building name or number of the address"
                },
                "postOfficeBox": {
                    "$ref": "#/definitions/fieldMetadata",
                    "description": "The post office box information of this address"
                },
                "locality": {
                    "$ref": "#/definitions/fieldMetadata",
                    "description": "The name of the city/town/village of this address"
                },
                "dependentLocality": {
                    "$ref": "#/definitions/fieldMetadata",
                    "description": "The name of the dependent (sub) locality of this address"
                },
                "region": {
                    "$ref": "#/definitions/fieldMetadata",
                    "description": "The name of the state/province/etc. of this address"
                },
                "subregion": {
                    "$ref": "#/definitions/fieldMetadata",
                    "description": "The name of the subregion of this address"
                },
                "subsubregion": {
                    "$ref": "#/definitions/fieldMetadata",
                    "description": "The name of the subsubregion of this address"
                },
                "postalCode": {
                    "$ref": "#/definitions/fieldMetadata",
                    "description": "The postal code/postcode/zip code of the address"
                },
                "postOffice": {
                    "$ref": "#/definitions/fieldMetadata",
                    "description": "The name of the post office of the address"
                },
                "country": {
                    "$ref": "#/definitions/fieldMetadata",
                    "description": "The name of the country of this address"
                }
            }
        },
